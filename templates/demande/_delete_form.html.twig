<form method="post" action="{{ path('demande_delete', {'numDem': demande.numDem}) }}" onsubmit="return confirm('Vous confirmez la suppression?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ demande.numDem) }}">
    <button class="btn">Supprimer</button>
</form>
